{
  "name": "samyan/omnipay-neteller",
  "type": "library",
  "description": "Neteller driver for the Omnipay payment processing library",
  "keywords": [
    "neteller",
    "gateway",
    "merchant",
    "omnipay",
    "pay",
    "payment"
  ],
  "homepage": "https://github.com/samyan/omnipay-neteller",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Omnipay\\Neteller\\": "src/"
    }
  },
  "require": {
    "omnipay/common": "^3.0"
  },
  "require-dev": {
    "omnipay/tests": "^3.0",
    "squizlabs/php_codesniffer": "3.*",
    "phpro/grumphp": "^0.15.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs -p --standard=PSR2 src/",
    "fix-style": "phpcbf -p --standard=PSR2 src/"
  },
  "prefer-stable": true
}